{
  "name": "staybae-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "test:unit": "NODE_ENV=development vitest run",
    "coverage": "NODE_ENV=development vitest run --coverage",
    "prepare": "husky",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@badrap/bar-of-progress": "^0.2.2",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.0.18",
    "@mui/material": "^5.12.2",
    "@types/jwt-decode": "^3.1.0",
    "@vitest/coverage-istanbul": "^1.2.2",
    "axios": "^1.3.6",
    "concurrent": "^0.3.2",
    "date-fns": "^2.30.0",
    "husky": "^9.0.10",
    "jwt-decode": "^3.1.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-progressive-graceful-image": "^0.7.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.2",
    "react-spinners": "^0.13.8",
    "sort-by": "^1.2.0",
    "vite-plugin-istanbul": "^5.0.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@emotion/jest": "^11.10.8",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.4",
    "@types/react": "^18.0.28",
    "@types/react-date-range": "^1.4.4",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.17",
    "blob-polyfill": "^7.0.20220408",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "jest": "^29.5.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^13.2.1",
    "msw": "^2.1.6",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^1.2.2",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,css}": [
      "bash -c tsc -p tsconfig --noEmit",
      "eslint --cache --fix --max-warnings=0"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setUpTests.ts"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
